{
  "name": "timebox",
  "version": "1.0.0",
  "description": "SSG/SSR for timebox github pages",
  "type": "module",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "copy:static": "cp -r ./dist/static ./out",
    "copy:generator": "cp ./old_generate.html ./out",
    "copy:assets:png": "cp ./*.png ./out",
    "copy:assets:jpg": "cp ./*.jpg ./out",
    "copy:assets:mjs": "cp ./*.mjs ./out",
    "copy:assets:ico": "cp ./*.ico ./out",
    "copy:assets": "npm run copy:assets:png && npm run copy:assets:jpg && npm run copy:assets:mjs && npm run copy:assets:ico",
    "fix:assets:path": "sed -i '' 's|/assets/|/timebox/assets/|g' ./out/*.html",
    "copy": "npm run copy:static && npm run copy:generator && npm run copy:assets",
    "watch:server": "vite build --ssr src/entry-server.tsx --watch --outDir dist/server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "dev": "vite",
    "serve": "cross-env NODE_ENV=dev node server"
  },
  "repository": "git@github.com:eugeny-dementev/timebox.git",
  "author": "Eugeny Dementev <eugeny.dementev@ya.ru>",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "style-to-object": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.72",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "postcss": "^8.4.38",
    "serve-static": "^2.0.0-beta.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6"
  }
}
